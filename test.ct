// Funcao media que retorna um real

funcao real media (inteiro a) {
  inteiro i
  inteiro soma
  inteiro quantidade
  real retorno

  soma := 0
  quantidade := 0

  para (i := 1; i <= a; i++) {
    quantidade++
    soma := soma + i
  }
}

